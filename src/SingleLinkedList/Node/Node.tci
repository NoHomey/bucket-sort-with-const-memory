#pragma once

#include "Node.thd"
#include <utility>

template<typename T>
inline SingleLinkedList<T>::Node::Node(const T& value, Node* next)
: next{next}, value{value} { }

template<typename T>
inline SingleLinkedList<T>::Node::Node(T&& value, Node* next) noexcept
: next{next}, value{std::move(value)} { }